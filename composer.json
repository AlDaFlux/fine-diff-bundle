{
  "name": "webtown-php/fine-diff-bundle",
  "type": "symfony-bundle",
  "version": "1.2",
  "description": "gorhill/PHP-FineDiff Symfony2 Bundle implementation",
  "keywords": [
    "diff",
    "string compare"
  ],
  "homepage": "https://github.com/webtown-php/fine-diff-bundle",
  "license": "MIT",
  "require": {
    "php": ">=5.4",
    "gorhill/php-fine-diff":    "~0.1.0",
    "symfony/finder":           "~2.3 || ~3.0",
    "symfony/filesystem":       "~2.3 || ~3.0",
    "symfony/options-resolver": "~2.3 || ~3.0",
    "symfony/framework-bundle": "~2.3 || ~3.0",
    "twig/twig":                "~1.16|| ~2.0"
  },
  "require-dev": {
    "phpunit/phpunit":          "~4.4"
  },
  "autoload": {
    "psr-4": {
      "WebtownPHP\\Bundle\\FineDiffBundle\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
