{
  "name": "greywolfs/fine-diff-bundle",
  "type": "symfony-bundle",
  "description": "gorhill/PHP-FineDiff Symfony3 Bundle implementation",
  "keywords": [
    "diff",
    "string compare"
  ],
  "homepage": "https://github.com/greywolfs/fine-diff-bundle",
  "license": "MIT",
  "require": {
    "php": ">=5.4",
    "gorhill/php-fine-diff":    "~0.1.2",
    "symfony/finder":           "~3.2",
    "symfony/filesystem":       "~3.2",
    "symfony/options-resolver": "~3.2",
    "symfony/framework-bundle": "~3.2"
  },
  "require-dev": {
    "phpunit/phpunit":          "~4.4"
  },
  "autoload": {
    "psr-4": {
      "WebtownPHP\\Bundle\\FineDiffBundle\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.4-dev"
    }
  }
}
